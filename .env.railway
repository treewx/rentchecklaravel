APP_NAME="Rent Tracker"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://your-railway-app.railway.app

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

# PostgreSQL (Railway will inject these from the database service)
DB_CONNECTION=pgsql
DB_HOST=${PGHOST}
DB_PORT=${PGPORT}
DB_DATABASE=${PGDATABASE}
DB_USERNAME=${PGUSER}
DB_PASSWORD=${PGPASSWORD}

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Email configuration for production
MAIL_MAILER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@your-domain.com
MAIL_FROM_NAME="Rent Tracker"

# Akahu Production Credentials
AKAHU_CLIENT_ID=your-production-client-id
AKAHU_CLIENT_SECRET=your-production-client-secret
AKAHU_API_BASE_URL=https://api.akahu.io/v1